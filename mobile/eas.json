{
  "cli": {
    "version": ">= 5.0.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "emulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "certificateType": "distribution"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "ios": {
        "certificateType": "distribution",
        "signingCertificate": "production"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": 123456789,
        "appleTeamId": "ABCDEFG123"
      },
      "android": {
        "serviceAccount": "./android-service-account.json",
        "track": "production",
        "releaseStatus": "completed"
      }
    }
  }
}
